{
	"name": "Artesgo Bookmarks",
	"description": "My own bookmarks extension",
	"version": "1.2",
	"manifest_version": 3,
	"permissions": ["bookmarks", "storage"],
	"action": {
		"default_popup": "index.html#popup"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {
		"16": "images/bookmark--16.png",
		"32": "images/bookmark--32.png",
		"48": "images/bookmark--48.png",
		"64": "images/bookmark--64.png",
		"128": "images/bookmark--128.png"
	}
}
